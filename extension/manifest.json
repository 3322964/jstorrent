{

    "name": "JSTorrent contextmenu extension",
    "description": "Javascript Torrent Client helper extension",
    "version": "0.0.5",
    "manifest_version": 2,
    "permissions": [
        "contextMenus",
        "notifications",
        "*://*.jstorrent.com/*",
        "*://jstorrent.com/*"
    ],
    "externally_connectable": {
      "ids": [ 
          "anhdpjpojoipgpmfanmedjghaligalgb",
          "abmohcnlldaiaodkpacnldcdnjjgldfh"
       ],
      "matches": [
          "*://*.jstorrent.com/*"
      ]
    },
    "background": {
        "scripts": ["background.js"]
    },
    "icons": { 
        "16": "icon16.png", 
        "48": "icon48.png", 
        "128": "icon128.png" 
    }
}
